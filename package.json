{
  "name": "directus-extension-directus-operation-slugify",
  "description": "Generates a language aware slug from texts.",
  "icon": "extension",
  "version": "v1.0.0",
  "keywords": [
    "directus",
    "directus-extension",
    "directus-custom-operation",
    "slugify"
  ],
  "type": "module",
  "author": "Murat GÃ¶zel <murat@gozel.com.tr>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/muratgozel/directus-operation-slugify.git"
  },
  "bugs": {
    "url": "https://github.com/muratgozel/directus-operation-slugify/issues"
  },
  "homepage": "https://github.com/muratgozel/directus-operation-slugify#readme",
  "directus:extension": {
    "type": "operation",
    "path": {
      "app": "dist/app.js",
      "api": "dist/api.js"
    },
    "source": {
      "app": "src/app.ts",
      "api": "src/api.ts"
    },
    "host": "^10.3.0"
  },
  "scripts": {
    "build": "directus-extension build",
    "dev": "directus-extension build -w --no-minify",
    "link": "directus-extension link",
    "up": "npm run build && aws s3 sync ./ s3://[S3_BUCKET_NAME]/extensions/directus-extension-operation-slugify/ --exclude '*' --include 'package.json' --include 'dist/*' --delete",
    "up-dev": "npm run build && rsync -a --exclude='src' --exclude='node_modules' --include='package.json' --include='dist' --include='dist/*' --exclude='*' --delete ./* [EXTENSIONS_DIR]/extensions/directus-extension-operation-slugify/"
  },
  "license": "MIT",
  "devDependencies": {
    "@directus/extensions-sdk": "10.3.0",
    "@types/node": "^20.11.16",
    "@types/speakingurl": "^13.0.6",
    "node-releaser": "^2.1.4",
    "typescript": "^5.3.3",
    "vue": "^3.4.15"
  },
  "dependencies": {
    "speakingurl": "^14.0.1"
  }
}